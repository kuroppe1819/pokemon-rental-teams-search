{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "astro build",
    "start:astro": "astro dev",
    "start:cloudflare": "npm run build && wrangler pages dev --persist ./dist",
    "start:astro-with-cloudflare": "wrangler pages dev --persist --proxy -- astro dev",
    "watch": "npm-watch build",
    "deploy:production": "npm run build && wrangler pages publish dist",
    "db:init": "wrangler d1 execute pokemon-rental-teams --local --file=./../schema.sql"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx,astro",
      "runOnChangeOnly": true
    }
  },
  "dependencies": {
    "@astrojs/cloudflare": "^5.0.0",
    "astro": "^1.8.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "npm-watch": "^0.11.0",
    "typescript": "^4.9.4",
    "wrangler": "2.6.2"
  }
}
