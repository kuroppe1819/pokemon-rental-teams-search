{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build": "npm run build:css && remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "remix build && run-p \"dev:*\"",
    "deploy:prod": "npm run build && wrangler pages publish ./public",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc -b",
    "wrangler": "wrangler pages dev ./public --d1=DB --persist",
    "db:init": "wrangler d1 execute pokemon-rental-teams --local --file=./../schemas/init.sql",
    "db:insert:pokemon": "wrangler d1 execute pokemon-rental-teams --local --file=./../schemas/insert_pokemon_name.sql"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.10.1",
    "@remix-run/cloudflare-pages": "^1.10.1",
    "@remix-run/react": "^1.10.1",
    "cross-env": "^7.0.3",
    "flowbite": "^1.6.2",
    "flowbite-react": "^0.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@remix-run/dev": "^1.10.1",
    "@remix-run/eslint-config": "^1.10.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "wrangler": "^2.7.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
